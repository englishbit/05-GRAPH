#include<stdio.h>
#include<iostream>
using namespace std;
#define MAX 100000
int matrix[MAX][MAX]={0};

int main(){
    int node,edge;
    cin>>node;
    cin>>edge;

    int x,y;
    for(int i=0;i<edge;i++){
        cin>>x;
        cin>>y;

        matrix[x][y]=1;
        matrix[y][x]=1;
    }

    for(int i=0;i<node;i++){
        for(int j=0;j<node;j++){
            cout<<matrix[i][j];
        }
        cout<<endl;
    }

    return 0;
}